# Development Environment Configuration
# Copy to .env in project root for local development

# Environment
NODE_ENV=development
VITAL_ENVIRONMENT=dev

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/vital_dev
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Services
OPENAI_API_KEY=sk-your-openai-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# LangSmith (Optional - for monitoring)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your-langsmith-key-here
LANGCHAIN_PROJECT=vital-dev

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# Application
PORT=3000
API_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3000

# Feature Flags
ENABLE_DEBUG_LOGGING=true
ENABLE_ANALYTICS=false
ENABLE_MONITORING=false

# Security
JWT_SECRET=your-dev-jwt-secret-here
SESSION_SECRET=your-dev-session-secret-here

# Rate Limiting
RATE_LIMIT_ENABLED=false
RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_WINDOW_MS=60000

