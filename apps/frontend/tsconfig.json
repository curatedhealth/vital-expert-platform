{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/ui/*": [
        "../../packages/ui/src/*"
      ],
      "@/core/*": [
        "../../packages/core/src/*"
      ],
      "@/shared/*": [
        "./shared/*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/utils/*": [
        "./shared/utils/*"
      ],
      "@/features/*": [
        "./features/*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@/config/*": [
        "./config/*"
      ],
      "@/contexts/*": [
        "./contexts/*"
      ],
      "@/agents/*": [
        "./agents/*"
      ],
      "@/middleware/*": [
        "./middleware/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "features/**/*.ts",
    "features/**/*.tsx",
    "components/**/*.ts",
    "components/**/*.tsx",
    "shared/**/*.ts",
    "shared/**/*.tsx",
    "hooks/**/*.ts",
    "hooks/**/*.tsx",
    "lib/**/*.ts",
    "lib/**/*.tsx",
    "types/**/*.ts",
    "types/**/*.tsx",
    "app/**/*.ts",
    "app/**/*.tsx",
    "contexts/**/*.ts",
    "contexts/**/*.tsx",
    "config/**/*.ts",
    "config/**/*.tsx",
    "agents/**/*.ts",
    "agents/**/*.tsx",
    "middleware/**/*.ts",
    "middleware/**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next",
    "out",
    "dist",
    "build"
  ]
}