{
  "environment": "development",
  "version": "1.0.0",
  "lastUpdated": "2024-09-25",

  "application": {
    "name": "VITAL Path Healthcare AI",
    "baseUrl": "http://localhost:3000",
    "apiVersion": "v1",
    "debugMode": true,
    "verboseLogging": true
  },

  "database": {
    "host": "localhost",
    "port": 54322,
    "name": "vital_path_dev",
    "ssl": false,
    "connectionTimeout": 5000,
    "maxConnections": 10
  },

  "security": {
    "enforceHttps": false,
    "corsEnabled": true,
    "corsOrigins": ["http://localhost:3000", "http://localhost:3001"],
    "rateLimiting": {
      "enabled": false,
      "requests": 1000,
      "window": "1h"
    },
    "jwtExpiration": "24h"
  },

  "healthcare": {
    "hipaaMode": "development",
    "phiLogging": false,
    "auditLevel": "basic",
    "encryptionRequired": false,
    "mockPatientData": true,
    "medicalValidation": {
      "enabled": true,
      "strictMode": false,
      "pharmaFramework": {
        "enabled": true,
        "threshold": 0.7
      }
    }
  },

  "ai": {
    "providers": {
      "openai": {
        "enabled": true,
        "model": "gpt-4",
        "maxTokens": 2000,
        "temperature": 0.1
      }
    },
    "medicalSafety": {
      "confidenceThreshold": 0.8,
      "requireHumanReview": true,
      "harmfulnessCheck": true
    },
    "rag": {
      "enabled": true,
      "vectorStore": "supabase",
      "embeddingModel": "text-embedding-ada-002",
      "maxDocuments": 10
    }
  },

  "features": {
    "clinicalAgents": true,
    "promptGeneration": true,
    "knowledgeBase": true,
    "analytics": true,
    "complianceMonitoring": true,
    "realTimeCollaboration": false
  },

  "logging": {
    "level": "debug",
    "format": "detailed",
    "destinations": ["console", "file"],
    "medicalAudit": {
      "enabled": true,
      "level": "info",
      "includeUserActions": true,
      "includeMedicalQueries": true
    }
  },

  "testing": {
    "mockExternalAPIs": true,
    "seedTestData": true,
    "enableTestRoutes": true,
    "bypassAuthentication": false
  }
}