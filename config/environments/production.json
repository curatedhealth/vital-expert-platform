{
  "environment": "production",
  "version": "1.0.0",
  "lastUpdated": "2024-09-25",

  "application": {
    "name": "VITAL Path Healthcare AI",
    "baseUrl": "${NEXT_PUBLIC_APP_URL}",
    "apiVersion": "v1",
    "debugMode": false,
    "verboseLogging": false
  },

  "database": {
    "host": "${DATABASE_HOST}",
    "port": "${DATABASE_PORT}",
    "name": "${DATABASE_NAME}",
    "ssl": true,
    "connectionTimeout": 10000,
    "maxConnections": 50,
    "pooling": {
      "enabled": true,
      "min": 2,
      "max": 20
    }
  },

  "security": {
    "enforceHttps": true,
    "corsEnabled": true,
    "corsOrigins": ["${NEXT_PUBLIC_APP_URL}"],
    "rateLimiting": {
      "enabled": true,
      "requests": 100,
      "window": "1h"
    },
    "jwtExpiration": "1h",
    "encryption": {
      "algorithm": "AES-256-GCM",
      "keyRotation": "30d"
    },
    "headers": {
      "hsts": true,
      "csp": true,
      "xframeOptions": "DENY"
    }
  },

  "healthcare": {
    "hipaaMode": "strict",
    "phiLogging": false,
    "auditLevel": "comprehensive",
    "encryptionRequired": true,
    "mockPatientData": false,
    "dataRetention": {
      "auditLogs": "7 years",
      "medicalData": "permanent",
      "userSessions": "30 days"
    },
    "medicalValidation": {
      "enabled": true,
      "strictMode": true,
      "pharmaFramework": {
        "enabled": true,
        "threshold": 0.85
      },
      "fdaCompliance": {
        "enabled": true,
        "validationRequired": true
      }
    }
  },

  "ai": {
    "providers": {
      "openai": {
        "enabled": true,
        "model": "gpt-4",
        "maxTokens": 1500,
        "temperature": 0.0
      }
    },
    "medicalSafety": {
      "confidenceThreshold": 0.9,
      "requireHumanReview": true,
      "harmfulnessCheck": true,
      "multiStageValidation": true,
      "fallbackToHuman": true
    },
    "rag": {
      "enabled": true,
      "vectorStore": "supabase",
      "embeddingModel": "text-embedding-ada-002",
      "maxDocuments": 5,
      "sourceValidation": true
    }
  },

  "features": {
    "clinicalAgents": true,
    "promptGeneration": true,
    "knowledgeBase": true,
    "analytics": true,
    "complianceMonitoring": true,
    "realTimeCollaboration": true,
    "advancedReporting": true
  },

  "logging": {
    "level": "info",
    "format": "json",
    "destinations": ["file", "external"],
    "medicalAudit": {
      "enabled": true,
      "level": "comprehensive",
      "includeUserActions": true,
      "includeMedicalQueries": true,
      "tamperProof": true,
      "digitalSignatures": true
    },
    "external": {
      "enabled": true,
      "endpoint": "${AUDIT_LOG_ENDPOINT}",
      "encryption": true
    }
  },

  "monitoring": {
    "healthChecks": {
      "enabled": true,
      "interval": "30s",
      "endpoints": ["/api/health", "/api/health/db", "/api/health/ai"]
    },
    "metrics": {
      "enabled": true,
      "medicalResponseTime": true,
      "complianceMetrics": true,
      "userEngagement": true
    },
    "alerts": {
      "enabled": true,
      "channels": ["email", "slack"],
      "medicalSafety": {
        "lowConfidence": true,
        "harmfulResponse": true,
        "complianceViolation": true
      }
    }
  },

  "backup": {
    "enabled": true,
    "schedule": "daily",
    "retention": "90 days",
    "encryption": true,
    "offsite": true
  },

  "performance": {
    "caching": {
      "enabled": true,
      "ttl": "1h",
      "excludePatientData": true
    },
    "compression": true,
    "cdn": {
      "enabled": true,
      "staticAssets": true
    }
  },

  "testing": {
    "mockExternalAPIs": false,
    "seedTestData": false,
    "enableTestRoutes": false,
    "bypassAuthentication": false
  }
}