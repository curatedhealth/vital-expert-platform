{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AI Agent Complete Data Model",
  "description": "Comprehensive gold standard schema for AI agents with all fields for import/export",
  "version": "2.0",
  "type": "object",
  "required": ["name", "display_name", "tier", "status"],
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid",
      "description": "Unique agent identifier (auto-generated if not provided)"
    },
    "name": {
      "type": "string",
      "description": "Machine-readable name (lowercase, hyphenated)",
      "pattern": "^[a-z0-9-]+$",
      "examples": ["clinical-trial-designer", "patient-education-assistant"]
    },
    "display_name": {
      "type": "string",
      "description": "Human-readable display name",
      "examples": ["Clinical Trial Designer", "Patient Education Assistant"]
    },
    "description": {
      "type": "string",
      "description": "Brief description of the agent (1-2 sentences)",
      "maxLength": 500
    },
    "tier": {
      "type": "integer",
      "enum": [1, 2, 3],
      "description": "Agent complexity tier: 1 (Foundational), 2 (Specialist), 3 (Expert)"
    },
    "status": {
      "type": "string",
      "enum": ["active", "beta", "deprecated", "draft"],
      "description": "Current deployment status"
    },
    "priority": {
      "type": "integer",
      "minimum": 1,
      "maximum": 10,
      "description": "Priority level (1-10, higher = more critical)"
    },
    "avatar": {
      "type": "string",
      "description": "Avatar icon identifier or path",
      "examples": ["medical-doctor.svg", "regulatory-expert.png"]
    },
    "model": {
      "type": "string",
      "description": "Primary LLM model to use",
      "enum": [
        "gpt-4",
        "gpt-4-turbo",
        "gpt-4-turbo-preview",
        "gpt-4o",
        "claude-3-opus",
        "claude-3-sonnet",
        "claude-3-haiku"
      ],
      "default": "gpt-4-turbo-preview"
    },
    "system_prompt": {
      "type": "string",
      "description": "Complete system prompt (can be generated via template or provided)",
      "minLength": 100
    },
    "capabilities": {
      "type": "array",
      "description": "List of agent capabilities (minimum 5 for production)",
      "items": {
        "type": "string"
      },
      "minItems": 5,
      "examples": [
        [
          "Clinical trial protocol design",
          "FDA regulatory compliance",
          "Statistical analysis planning",
          "Endpoint selection",
          "Sample size calculation"
        ]
      ]
    },
    "tools": {
      "type": "array",
      "description": "Tools available to the agent",
      "items": {
        "type": "object",
        "required": ["name"],
        "properties": {
          "id": {
            "type": "string",
            "description": "Tool unique identifier"
          },
          "name": {
            "type": "string",
            "description": "Tool name"
          },
          "description": {
            "type": "string",
            "description": "What the tool does"
          },
          "type": {
            "type": "string",
            "enum": ["retrieval", "action", "computation", "integration"],
            "description": "Tool category"
          }
        }
      }
    },
    "metadata": {
      "type": "object",
      "description": "Complete agent metadata and configuration",
      "properties": {
        "architecture_pattern": {
          "type": "string",
          "enum": ["REACTIVE", "DELIBERATIVE", "HYBRID", "MULTI_AGENT"],
          "description": "Agent architecture pattern",
          "default": "REACTIVE"
        },
        "reasoning_method": {
          "type": "string",
          "enum": ["DIRECT", "COT", "REACT"],
          "description": "Primary reasoning approach",
          "default": "DIRECT"
        },
        "communication_tone": {
          "type": "string",
          "description": "Communication tone (e.g., Professional, Friendly, Empathetic)",
          "examples": ["Friendly and Professional", "Clinical and Precise", "Empathetic and Supportive"]
        },
        "communication_style": {
          "type": "string",
          "description": "Communication style (e.g., Concise, Detailed, Conversational)",
          "examples": ["Clear and accessible", "Detailed and structured", "Conversational"]
        },
        "primary_mission": {
          "type": "string",
          "description": "Core mission statement for the agent",
          "examples": ["Design scientifically robust clinical trial protocols"]
        },
        "value_proposition": {
          "type": "string",
          "description": "Key value delivered to users",
          "examples": ["Reduces protocol amendments by 40% through comprehensive upfront design"]
        },
        "business_function": {
          "type": "string",
          "description": "Organizational business function",
          "examples": ["Clinical Development", "Regulatory Affairs", "Commercial"]
        },
        "department": {
          "type": "string",
          "description": "Department within business function",
          "examples": ["Clinical Operations", "Medical Affairs", "Compliance"]
        },
        "role_name": {
          "type": "string",
          "description": "Specific role within department",
          "examples": ["Protocol Designer", "Medical Writer", "Regulatory Strategist"]
        },
        "medical_specialty": {
          "type": "string",
          "description": "Medical specialty if applicable",
          "examples": ["Oncology", "Cardiology", "Neurology", "General Medicine"]
        },
        "knowledge_domains": {
          "type": "array",
          "description": "Knowledge domains the agent has access to",
          "items": {
            "type": "string"
          },
          "examples": [
            ["FDA Guidance Documents", "ICH-GCP Guidelines", "Statistical Methods"]
          ]
        },
        "safety_compliance": {
          "type": "object",
          "description": "Safety and compliance framework",
          "properties": {
            "prohibitions": {
              "type": "array",
              "description": "Absolute prohibitions (what agent must NEVER do)",
              "items": {
                "type": "string"
              },
              "examples": [
                [
                  "Providing definitive medical diagnoses",
                  "Recommending specific treatments without medical oversight",
                  "Accessing or requesting protected health information (PHI)",
                  "Overriding medical advice from healthcare providers"
                ]
              ]
            },
            "mandatory_protections": {
              "type": "array",
              "description": "Mandatory protections (what agent must ALWAYS do)",
              "items": {
                "type": "string"
              },
              "examples": [
                [
                  "Always prioritize user safety",
                  "Maintain privacy in all interactions",
                  "Provide evidence-based information only",
                  "Escalate complex cases appropriately"
                ]
              ]
            },
            "regulatory_standards": {
              "type": "array",
              "description": "Applicable regulatory standards",
              "items": {
                "type": "string"
              },
              "examples": [
                ["HIPAA", "FDA Guidelines", "ICH-GCP", "GDPR"]
              ]
            },
            "confidence_thresholds": {
              "type": "object",
              "description": "Confidence level thresholds for actions",
              "properties": {
                "minimum_confidence": {
                  "type": "integer",
                  "minimum": 0,
                  "maximum": 100,
                  "description": "Minimum confidence to provide response (add disclaimers below this)",
                  "default": 75
                },
                "escalation_threshold": {
                  "type": "integer",
                  "minimum": 0,
                  "maximum": 100,
                  "description": "Confidence below which to escalate to human expert",
                  "default": 70
                },
                "defer_to_human": {
                  "type": "integer",
                  "minimum": 0,
                  "maximum": 100,
                  "description": "Confidence below which to refuse and refer to human",
                  "default": 60
                }
              }
            }
          }
        },
        "performance_targets": {
          "type": "object",
          "description": "Performance metrics and targets",
          "properties": {
            "accuracy_target": {
              "type": "integer",
              "minimum": 0,
              "maximum": 100,
              "description": "Target accuracy percentage (Tier 1: 85%, Tier 2: 90%, Tier 3: 95%)",
              "default": 85
            },
            "response_time_target": {
              "type": "number",
              "minimum": 0,
              "description": "Target response time in seconds",
              "default": 2
            },
            "user_satisfaction_target": {
              "type": "number",
              "minimum": 0,
              "maximum": 5,
              "description": "Target user satisfaction score (out of 5)",
              "default": 4.0
            },
            "escalation_rate_target": {
              "type": "integer",
              "minimum": 0,
              "maximum": 100,
              "description": "Target escalation rate percentage (Tier 1: <15%, Tier 2: <10%, Tier 3: <5%)",
              "default": 15
            }
          }
        },
        "model_justification": {
          "type": "string",
          "description": "Justification for model selection",
          "examples": [
            "GPT-4 Turbo selected for Tier 1 foundational agents - optimal balance of speed, cost, and accuracy for high-volume general-purpose use cases"
          ]
        },
        "model_citation": {
          "type": "string",
          "description": "Citation/reference for model selection",
          "examples": [
            "OpenAI GPT-4 Turbo (gpt-4-turbo-preview) - Fast inference, strong general capabilities, cost-effective for high volume"
          ]
        },
        "evidence_based": {
          "type": "boolean",
          "description": "Whether agent is configured with evidence-based approach",
          "default": true
        },
        "fda_samd_class": {
          "type": "string",
          "enum": ["I", "II", "III"],
          "description": "FDA Software as Medical Device classification (if applicable)"
        },
        "hipaa_compliant": {
          "type": "boolean",
          "description": "Whether agent is HIPAA compliant",
          "default": false
        },
        "pharma_protocol": {
          "type": "boolean",
          "description": "Whether agent follows pharmaceutical protocols",
          "default": false
        },
        "verify_protocol": {
          "type": "boolean",
          "description": "Whether agent follows VERIFY protocol",
          "default": false
        },
        "last_updated": {
          "type": "string",
          "format": "date-time",
          "description": "Last update timestamp"
        }
      }
    },
    "prompt_starters": {
      "type": "array",
      "description": "Example prompts to help users get started",
      "items": {
        "type": "object",
        "required": ["text"],
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for prompt starter"
          },
          "text": {
            "type": "string",
            "description": "Prompt text",
            "examples": ["Design a phase III oncology trial protocol"]
          },
          "icon": {
            "type": "string",
            "description": "Emoji or icon for the prompt",
            "examples": ["ðŸ“‹", "ðŸ”", "ðŸ’¡", "âš•ï¸"]
          }
        }
      },
      "minItems": 4,
      "maxItems": 6
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "description": "Creation timestamp"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "description": "Last update timestamp"
    }
  },
