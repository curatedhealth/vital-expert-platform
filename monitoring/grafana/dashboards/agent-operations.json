{
  "dashboard": {
    "title": "Agent Operations - Real-Time Monitoring",
    "tags": ["agents", "monitoring", "graphrag", "search", "performance"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Agent Search Duration - P95",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_search_duration_ms_bucket[5m]))",
            "legendFormat": "{{operation}} - {{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Milliseconds",
            "format": "ms"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [1000],
                "type": "gt"
              },
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "Agent Search Slow",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "title": "Agent Search Operations Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agent_search_total[5m])",
            "legendFormat": "{{operation}} - {{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Ops/sec",
            "format": "ops"
          }
        ]
      },
      {
        "id": 3,
        "title": "GraphRAG vs Fallback Usage",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 8, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum by (method) (rate(agent_search_total[5m]))",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Agent Search Error Rate",
        "type": "graph",
        "gridPos": {"x": 8, "y": 8, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "rate(agent_search_errors_total[5m])",
            "legendFormat": "{{operation}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Errors/sec",
            "format": "ops"
          }
        ]
      },
      {
        "id": 5,
        "title": "GraphRAG Hit Rate",
        "type": "stat",
        "gridPos": {"x": 16, "y": 8, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "rate(graphrag_search_hits_total[5m]) / (rate(graphrag_search_hits_total[5m]) + rate(graphrag_search_fallback_total[5m]))",
            "refId": "A",
            "format": "percentunit"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Fallback Usage Rate",
        "type": "stat",
        "gridPos": {"x": 16, "y": 12, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "rate(graphrag_search_fallback_total[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Agent Selection Performance",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_selection_duration_ms_bucket[5m]))",
            "legendFormat": "{{mode}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Milliseconds",
            "format": "ms"
          }
        ]
      },
      {
        "id": 8,
        "title": "Agent Selection Success Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agent_selection_total{confidence_level=\"high\"}[5m]) / rate(agent_selection_total[5m])",
            "legendFormat": "High Confidence",
            "refId": "A"
          },
          {
            "expr": "rate(agent_selection_total{confidence_level=\"medium\"}[5m]) / rate(agent_selection_total[5m])",
            "legendFormat": "Medium Confidence",
            "refId": "B"
          },
          {
            "expr": "rate(agent_selection_total{confidence_level=\"low\"}[5m]) / rate(agent_selection_total[5m])",
            "legendFormat": "Low Confidence",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "label": "Percentage",
            "format": "percentunit"
          }
        ]
      },
      {
        "id": 9,
        "title": "Search Results Count",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "agent_search_results_count",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Agents",
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "title": "Mode 2 & Mode 3 Execution Duration",
        "type": "graph",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mode2_execution_duration_ms_bucket[5m]))",
            "legendFormat": "Mode 2 P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(mode3_execution_duration_ms_bucket[5m]))",
            "legendFormat": "Mode 3 P95",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "Milliseconds",
            "format": "ms"
          }
        ]
      },
      {
        "id": 11,
        "title": "User Agent Operations",
        "type": "graph",
        "gridPos": {"x": 0, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(user_agent_operations_total[5m])",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Ops/sec",
            "format": "ops"
          }
        ]
      },
      {
        "id": 12,
        "title": "Agent Ranking Score Distribution",
        "type": "heatmap",
        "gridPos": {"x": 12, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agent_ranking_score_bucket[5m])",
            "format": "heatmap",
            "refId": "A"
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "schemaVersion": 27,
    "version": 1
  }
}

