{
  "dashboard": {
    "title": "RAG Operations - Real-Time Monitoring",
    "tags": ["rag", "monitoring", "latency", "cost"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "RAG Latency - P95 (SLO: <2000ms)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rag_latency_p95_milliseconds",
            "legendFormat": "P95 Latency",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Milliseconds",
            "format": "ms"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [2000],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "1m",
          "handler": 1,
          "name": "RAG P95 Latency Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "title": "RAG Queries Per Minute",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(rag_queries_total[1m]) * 60",
            "legendFormat": "Queries/min",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Queries",
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rag_cache_hit_rate * 100",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 20, "color": "yellow"},
              {"value": 30, "color": "green"}
            ]
          }
        }
      },
      {
        "id": 4,
        "title": "Daily Budget Usage",
        "type": "stat",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rag_budget_daily_usage_percent",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 80, "color": "yellow"},
              {"value": 100, "color": "red"}
            ]
          }
        }
      },
      {
        "id": 5,
        "title": "Cost Per Query",
        "type": "stat",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rag_cost_per_query_usd",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "currencyUSD",
          "decimals": 4,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.05, "color": "yellow"},
              {"value": 0.10, "color": "red"}
            ]
          }
        }
      },
      {
        "id": 6,
        "title": "Total Cost (Last Hour)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "increase(rag_cost_total_usd[1h])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "currencyUSD",
          "decimals": 2
        }
      },
      {
        "id": 7,
        "title": "Circuit Breaker States",
        "type": "bargauge",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "rag_circuit_breaker_state",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "minVizWidth": 0,
          "minVizHeight": 10,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 2, "color": "red"}
            ]
          }
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "CLOSED"},
              {"type": "value", "value": "1", "text": "HALF_OPEN"},
              {"type": "value", "value": "2", "text": "OPEN"}
            ]
          }
        }
      },
      {
        "id": 8,
        "title": "Component Latency Breakdown",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "rag_component_latency_milliseconds{component=\"query_embedding\"}",
            "legendFormat": "Query Embedding",
            "refId": "A"
          },
          {
            "expr": "rag_component_latency_milliseconds{component=\"vector_search\"}",
            "legendFormat": "Vector Search",
            "refId": "B"
          },
          {
            "expr": "rag_component_latency_milliseconds{component=\"reranking\"}",
            "legendFormat": "Cohere Re-ranking",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "label": "Milliseconds (P95)",
            "format": "ms"
          }
        ],
        "stack": true
      },
      {
        "id": 9,
        "title": "Cost by Provider (Last Hour)",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 18, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "increase(rag_cost_by_provider_usd[1h])",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        }
      },
      {
        "id": 10,
        "title": "Service Health - Success vs Failure Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 18, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(rag_circuit_breaker_successes_total[5m])",
            "legendFormat": "{{service}} - Success",
            "refId": "A"
          },
          {
            "expr": "rate(rag_circuit_breaker_failures_total[5m])",
            "legendFormat": "{{service}} - Failure",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "Requests/sec",
            "format": "reqps"
          }
        ]
      }
    ]
  }
}
