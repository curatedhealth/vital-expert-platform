{
  "name": "vital-path-digital-health-platform",
  "version": "1.0.0",
  "description": "VITAL Path Digital Health Intelligence Platform - 50+ Healthcare AI Agents",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:gateway": "pnpm --filter node-gateway dev",
    "dev:python": "pnpm --filter python-services dev",
    "build": "pnpm -r build",
    "build:frontend": "pnpm --filter frontend build",
    "build:gateway": "pnpm --filter node-gateway build",
    "start": "pnpm -r start",
    "start:frontend": "pnpm --filter frontend start",
    "start:gateway": "pnpm --filter node-gateway start",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "pnpm -r type-check",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "test:coverage": "pnpm -r test:coverage",
    "test:unit": "pnpm -r test:unit",
    "test:integration": "pnpm -r test:integration",
    "test:compliance": "pnpm -r test:compliance",
    "test:ci": "pnpm -r test:ci",
    "migrate": "node scripts/migration/migrate-runner.js",
    "migrate:run": "node scripts/migration/migrate-runner.js run",
    "migrate:status": "node scripts/migration/migrate-runner.js status",
    "migrate:validate": "node scripts/migration/migrate-runner.js validate",
    "db:migrate": "npm run migrate:run",
    "db:status": "npm run migrate:status",
    "db:validate": "npm run migrate:validate",
    "db:setup": "psql -f database/sql/run_all_migrations.sql",
    "db:schema": "psql -f database/sql/schema/main_schema.sql",
    "db:supabase:setup": "npx tsx scripts/setup-supabase.ts",
    "db:supabase:test": "npx tsx scripts/test-db-connection.ts",
    "compliance:scan": "node tools/compliance-scanner/hipaa-scanner.js",
    "medical:validate": "node tools/medical-validation/pharma-validator.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "healthcare:check": "npm run compliance:scan && npm run medical:validate",
    "healthcare:test": "npm run test:compliance && npm run healthcare:check",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "build:healthcare": "npm run healthcare:check && npm run build:production",
    "pre-commit": "npm run lint:fix && npm run format && npm run type-check",
    "pre-deploy": "npm run healthcare:test && npm run build:healthcare",
    "docs:generate": "typedoc --out docs/api src/",
    "clean": "rm -rf .next out coverage dist build",
    "reset": "npm run clean && npm install",
    "notion:export": "node scripts/export-to-notion-format.js",
    "notion:sync-to": "node scripts/sync-supabase-to-notion.js",
    "notion:sync-from": "node scripts/sync-from-notion.js",
    "notion:sync": "npm run notion:sync-from",
    "sync": "npm run notion:sync-from"
  },
