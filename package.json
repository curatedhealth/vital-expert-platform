{
  "name": "vital-path",
  "version": "1.0.0",
  "description": "VITAL Path - Digital Health Platform",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter @vital/digital-health-startup dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "clean": "pnpm -r clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "compliance:langchain": "node scripts/check-langchain-imports.js",
    "db:migrate": "cd apps/digital-health-startup && pnpm db:migrate",
    "db:status": "cd apps/digital-health-startup && pnpm db:status",
    "db:setup": "cd apps/digital-health-startup && pnpm db:setup",
    "healthcare:check": "cd apps/digital-health-startup && pnpm healthcare:check",
    "pre-commit": "pnpm lint && pnpm type-check && pnpm compliance:langchain",
    "pre-deploy": "pnpm build && pnpm test && pnpm compliance:langchain"
  },
  "devDependencies": {
    "@types/node": "^20.19.17",
    "dotenv": "^17.2.3",
    "prettier": "^3.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "critters": "^0.0.25",
    "next": "15",
    "pg": "^8.16.3",
    "react": "19",
    "react-dom": "19"
  }
}
