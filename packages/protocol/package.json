{
  "name": "@vital/protocol",
  "version": "1.0.0",
  "description": "Shared type definitions and contracts between VITAL frontend and backend",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "generate:json-schemas": "tsx src/generate-json-schemas.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist json-schemas"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "zod-to-json-schema": "^3.22.4"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src/json-schemas"
  ],
  "keywords": [
    "vital",
    "protocol",
    "types",
    "zod",
    "langgraph",
    "react-flow"
  ],
  "license": "UNLICENSED",
  "private": true
}
