{
  "dashboard": {
    "title": "AgentOS 3.0 - Performance Dashboard",
    "description": "Real-time performance monitoring for AgentOS including request rates, latency, and throughput",
    "tags": ["agentos", "performance", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (req/sec)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agentos_requests_total[5m])",
            "legendFormat": "{{service_type}} - {{tier}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"show": true, "placement": "bottom"},
          "tooltip": {"mode": "multi"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 2,
        "title": "Success Rate (%)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(agentos_requests_total{success=\"true\"}[5m])) / sum(rate(agentos_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Total Requests (1h)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(agentos_requests_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"}
          }
        }
      },
      {
        "id": 4,
        "title": "Response Time - P50 (Median)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(agentos_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{service_type}} - {{tier}} - P50",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"show": true, "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "Response Time - P95",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agentos_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{service_type}} - {{tier}} - P95",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"show": true, "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 30, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Response Time - P99",
        "type": "stat",
        "gridPos": {"x": 12, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(agentos_request_duration_seconds_bucket[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 30, "color": "yellow"},
                {"value": 120, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Requests by Tier",
        "type": "piechart",
        "gridPos": {"x": 18, "y": 4, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "sum by (tier) (increase(agentos_requests_total[1h]))",
            "legendFormat": "{{tier}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"show": true, "placement": "right"},
          "pieType": "pie",
          "displayLabels": ["percent", "name"]
        }
      },
      {
        "id": 8,
        "title": "Requests by Service Type",
        "type": "bargauge",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (service_type) (increase(agentos_requests_total[1h]))",
            "legendFormat": "{{service_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "continuous-GrYlRd"}
          }
        }
      },
      {
        "id": 9,
        "title": "Error Rate (errors/min)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agentos_requests_total{success=\"false\"}[5m]) * 60",
            "legendFormat": "{{service_type}} - Errors",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"show": true, "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "rpm",
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    }
  }
}

