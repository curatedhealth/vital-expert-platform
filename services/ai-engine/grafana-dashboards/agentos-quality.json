{
  "dashboard": {
    "title": "AgentOS 3.0 - Quality Dashboard",
    "description": "Quality metrics including confidence scores, accuracy, and evidence coverage",
    "tags": ["agentos", "quality", "accuracy"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Average Confidence Score",
        "type": "gauge",
        "gridPos": {"x": 0, "y": 0, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "avg(agentos_quality_confidence_score)",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.75, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Confidence Score by Tier",
        "type": "graph",
        "gridPos": {"x": 8, "y": 0, "w": 16, "h": 8},
        "targets": [
          {
            "expr": "avg by (tier) (agentos_quality_confidence_score)",
            "legendFormat": "{{tier}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"show": true, "placement": "bottom"},
          "tooltip": {"mode": "multi"}
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 3,
        "title": "Evidence Coverage Rate",
        "type": "stat",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(agentos_quality_evidence_coverage{has_evidence=\"true\"}) / sum(agentos_quality_evidence_coverage) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 98, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Escalation Rate",
        "type": "stat",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(agentos_escalations_total[1h]) * 3600",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 20, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Human Oversight Frequency",
        "type": "stat",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(agentos_human_oversight_total[1h])) * 3600",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 6,
        "title": "Tier 3 Usage (Deep Reasoning)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(agentos_requests_total{tier=\"tier_3\"}[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 7,
        "title": "Diagnostic Metrics (Last 24h)",
        "type": "table",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 10},
        "targets": [
          {
            "expr": "avg by (agent_id) (agentos_diagnostic_sensitivity)",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "avg by (agent_id) (agentos_diagnostic_specificity)",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "avg by (agent_id) (agentos_diagnostic_f1_score)",
            "format": "table",
            "refId": "C"
          },
          {
            "expr": "avg by (agent_id) (agentos_diagnostic_accuracy)",
            "format": "table",
            "refId": "D"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "Value #A": "Sensitivity",
                "Value #B": "Specificity",
                "Value #C": "F1 Score",
                "Value #D": "Accuracy"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*"},
              "properties": [
                {
                  "id": "unit",
                  "value": "percentunit"
                },
                {
                  "id": "decimals",
                  "value": 3
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"value": 0, "color": "red"},
                      {"value": 0.85, "color": "yellow"},
                      {"value": 0.90, "color": "green"}
                    ]
                  }
                },
                {
                  "id": "custom.displayMode",
                  "value": "gradient-gauge"
                }
              ]
            }
          ]
        }
      },
      {
        "id": 8,
        "title": "Confidence Distribution",
        "type": "histogram",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 10},
        "targets": [
          {
            "expr": "agentos_quality_confidence_score",
            "refId": "A"
          }
        ],
        "options": {
          "bucketSize": 0.1,
          "bucketOffset": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 9,
        "title": "Accuracy Trend (7 days)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 22, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "avg_over_time(agentos_diagnostic_accuracy[24h])",
            "legendFormat": "24h Rolling Average",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"show": true, "placement": "bottom"},
          "tooltip": {"mode": "single"}
        },
        "fieldConfig": {
          "defaults": {
            "min": 0.8,
            "max": 1.0,
            "unit": "percentunit",
            "color": {"mode": "palette-classic"}
          }
        }
      }
    ],
    "time": {"from": "now-24h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m"]
    }
  }
}

